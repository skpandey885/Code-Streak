/*package whatever //do not write package name here */
 
import java.io.*;
import java.util.*;
 
public class GFG {
	public static void main (String[] args) {
	 
	 Scanner input=new Scanner(System.in);
	 
	 int t=input.nextInt();
   
    while(t-->0){
    
    String s=input.next();
   
    char ch[]=s.toCharArray();
    Arrays.sort(ch);
     
    System.out.println(String.valueOf(ch));
     
 
    
    }
     
	}
}
