QUESTION LINK: https://leetcode.com/problems/a-number-after-a-double-reversal/

    //   TC: O(1) SC:O(1)

class Solution {
    public boolean isSameAfterReversals(int num) {
        if(num==0)
            return true;
        
        if(num%10==0)
            return false;
        
        return true;
        
    }
}
