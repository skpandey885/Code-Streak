https://leetcode.com/problems/a-number-after-a-double-reversal/

class Solution {
  public boolean isSameAfterReversals(int num) {
  
// TIME COMPLEXITY: O(1) & SPACE COMPLEXITY: O(1)

      if(num==0)
          return true;
      
   int rem=num%10;
      if(rem==0)
          return false;
      
      return true;
      
    }
}
