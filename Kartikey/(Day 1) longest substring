class Solution {
public:
    int lengthOfLongestSubstring(string s) {
        
        vector<int>  arr(256, -1);
        int ans = 0; 
        int start = -1;
        
        for(int i =0; i< s.size(); i++){
            if(arr[s[i]] > start)
                start = arr[s[i]];
            arr[s[i]] = i;
            ans = max(ans, i-start);
        }
        return ans;
    }
};
