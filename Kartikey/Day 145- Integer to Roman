//https://leetcode.com/problems/integer-to-roman/

class Solution {
public:
    string intToRoman(int num) {
        string result = "";  
        int nums[] = {1000,900,500,400,100,90,50,40,10,9,5,4,1}; 
        string romans[] = {"M","CM","D","CD","C","XC","L","XL","X","IX","V","IV","I"}; 
        
        for(int i=0; num!=0; i++){  
            while(num>=nums[i]){ 
                num-=nums[i]; 
                result+=romans[i]; 
            }   
        }
        return result;
    }
};
